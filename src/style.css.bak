  display: flex;
  align-items: center;
  justify-content: center;
}
.address_card p {
  font-size: 20px;
}
.address_card a.add_btn {
  color: #1d232c;
  font-size: 20px;
  font-weight: 700;
}
.address_card a.direction_btn {
  position: absolute;
  bottom: 70px;
  right: -133px;
  background: var(--main_color);
  border-radius: 100px;
  color: #fff;
  font-size: 18px;
  display: inline-block;
  padding: 10px 38px;
  opacity: 0;
  transition: 0.2s ease-in-out;
}

.address_card ul li.active {
  background: #1d232c;
  color: #fff;
}
.address_card ul li.active h2:after {
  background: #fff;
}
.address_card ul li.active a.add_btn {
  color: #fff;
}
.address_card ul li.active h2 i {
  background: #424242;
  border-color: #424242;
}
.address_card ul li.active a.direction_btn {
  opacity: 1;
}
/**************end address_blk**************/

/**************start footer**************/
