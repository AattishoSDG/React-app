/* DISCOVER CARD */
.discover-cards {
  padding-bottom: 50vh;
}
.discover-cards .row > *:nth-child(even) {
  position: relative;
  top: 140px;
}
.discover-card {
  border-radius: 24px;
  background: #fff;
  box-shadow: 0px 0px 32px 16px rgba(0, 0, 0, 0.04);
  padding: 35px;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  margin-bottom: 35px;
  position: relative;
  /* cursor: pointer; */
}
.discover-card::after {
  content: "\f061";
  position: absolute;
  width: 48px;
  height: 48px;
  flex-shrink: 0;
  border-radius: 50%;
  border: 1px solid #000;
  font-size: 2rem;
  display: flex;
  align-items: center;
  justify-content: center;
  font-family: "Font Awesome 6 Free";
  font-weight: 900;
  top: 100%;
  left: 50%;
  transform: translate(-50%, -50%) scale(0);
  visibility: hidden;
  opacity: 0;
  pointer-events: none;
  transition: all 0.5s;
}
.discover-card:hover::after {
  transform: translate(-50%, -50%) scale(1);
  visibility: visible;
  opacity: 1;
  pointer-events: none;
}
.discover-card .icon {
  width: 96px;
  height: 96px;
  display: flex;
  flex-shrink: 0;
  align-items: center;
  justify-content: center;
  background-color: rgba(247, 195, 90, 0.3);
  filter: drop-shadow(0px 0px 32px rgba(0, 0, 0, 0.04));
  margin-bottom: 1rem;
  border-radius: 50%;
}
.discover-card img {
  width: 40px;
  height: 40px;
  object-fit: cover;
  object-position: center;
}
.discover-card .title {
  color: #000;
  text-align: center;
  font-family: "DM Sans";
  font-size: 20px;
  font-style: normal;
  font-weight: 500;
  line-height: 48px; /* 240% */
  text-transform: capitalize;
  margin-bottom: 0.5rem;
}
.discover-card .text {
  color: #808080;
  text-align: center;
  font-family: "DM Sans";
  font-size: 16px;
  font-style: normal;
  font-weight: 400;
  line-height: 24px; /* 150% */
}
