  font-size: 64px;
  font-style: normal;
  font-weight: 600;
  line-height: 84px;
  text-decoration-line: underline;
  text-decoration-style: solid;
  text-decoration-skip-ink: none;
  text-decoration-thickness: auto;
  text-underline-offset: auto;
  text-underline-position: from-font;
}
.video-container {
  border-radius: 64px;
  overflow: hidden;
}
.section-home-hero .video-container {
  height: 750px;
}
.video-container video {
  width: 100%;
  height: 100%;
  display: block;
  object-fit: cover;
  object-position: center;
}
.section-home-hero p {
  color: #1d232c;
  font-family: "DM Sans";
  font-size: 16px;
  font-style: normal;
  font-weight: 300;
  line-height: 32px; /* 200% */
  display: flex;
  align-items: center;
  gap: 60px;
  max-width: 575px;
}
.section-home-hero p .scroll {
  position: relative;
  left: 0;
  top: 0;
  display: block;
  /* content: ""; */
  width: 40px;
  height: 76px;
  flex-shrink: 0;
  border: 1px solid #000;
  border-radius: 999px;
}
.section-home-hero p .scroll::after {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 1rem;
  display: block;
  content: "";
  width: 100%;
  height: 22px;
  background-image: url(/images/scroll-arrow.svg);
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
  flex-shrink: 0;
  /* border: 1px solid #000; */
  /* border-radius: 999px; */
}
