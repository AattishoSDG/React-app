* {
  box-sizing: border-box;
}
:root {
  --main_color: #038aba;

  --main_fort: "DM Sans", serif;
}
body {
  padding: 0 0 0 0;
  margin: 0 0 0 0;
  font-size: 16px;
  font-family: var(--main_fort);
  color: #000;
}
ul,
li {
  padding: 0;
  margin: 0;
  list-style-type: none;
}
img {
  max-width: 100%;
}
a {
  transition: 0.3s ease-in-out;
  text-decoration: none !important;
}

/* .container {
  max-width: 1352px;
} */
.pad_cmn_blk {
  padding-block: 75px;
}
.title_blk :is(h1, h2) {
  font-size: 48px;
  font-weight: 600;
}
.title_blk p {
  font-weight: 300;
}
.title-big h2 {
  font-size: 60px;
}
.title_blk h4 {
  text-transform: capitalize;
  font-weight: 400;
  line-height: 32px;
}
.title_blk h4 span,
.title_blk h4 span a {
  color: #5ec2ef;
}

/*************start header*************/
.header_nav {
  padding-block: 20px;
}
.header_nav ul {
  display: flex;
  gap: 40px;
}
.header_nav ul li {
  display: inline-block;
  margin: 0 5px;
}
.navmenu_mid li a {
  color: #1d232c;
}
.navmenu_mid li a:hover {
  color: var(--main_color);
}
.navmenu_end ul li:last-child a {
  border: 1px solid #1d232c;
  padding: 10px 20px;
  color: #1d232c;
  border-radius: 100px;
}
.navmenu_end ul li:last-child a:hover {
  background: #1d232c;
  color: #fff;
}
.navmenu_end ul li:first-child a {
  border-bottom: #1d232c 1px solid;
  padding-bottom: 3px;
  text-transform: capitalize;
  color: #1d232c;
}
.navmenu_end ul li:first-child a:hover {
  color: var(--main_color);
  border-bottom-color: var(--main_color);
}

/**************end header**************/

/**************start logo_blk**************/
.logo_card {
  background: #fff;
  box-shadow: 8px 8px 24px 0px rgba(0, 0, 0, 0.04);
  height: 165px;
  margin: 20px 30px;
}
/**************end logo_blk**************/

/**************start home_abt**************/
.home_abt_cnt p {
  line-height: 32px;
}
.home_abt_cnt ul {
  display: flex;
  margin-top: 50px;
}
.home_abt_cnt ul li h4 {
  display: block;
  font-size: 50px;
  font-weight: 300;
}
.home_abt_cnt ul li:not(:last-child) h4 {
  border-right: #000 1px solid;
}
.home_abt_cnt ul li:not(:last-child) {
  padding-right: 45px;
}
.home_abt_cnt ul li p {
  line-height: 27px;
}
.home_abt_cnt ul li:not(:last-child) p {
  padding-right: 20px;
}
.home_abt .title_blk {
  margin-bottom: 40px;
}
/**************end home_abt**************/

/**************start market_tab_blk**************/
.tabs-container ul {
  display: flex;
  text-align: center;
  margin-bottom: 105px;
  margin-top: 45px;
}
.tabs-container ul li {
  padding: 0px 15px 5px 15px;
  font-size: 20px;
  line-height: normal;
  color: #979797;
  position: relative;
  cursor: pointer;
}
.tabs-container ul li.active {
  color: #000;
}
.tabs-container ul li:after,
.tabs-container ul li:last-child::before {
  content: "";
  width: 1px;
  height: 3.1rem;
  background: #d0d0d0;
  position: absolute;
  top: 0;
}
.tabs-container ul li:after {
  left: 0;
}
.tabs-container ul li:last-child::before {
  right: 0;
}
.tabs-container .tabContent .desc {
  display: none;
  animation: fadeIn 0.5s ease-in-out;
}
.tabs-container .tabContent .desc.active {
  display: block;
}
@keyframes fadeIn {
  0% {
    opacity: 0;
    visibility: hidden;
  }
  100% {
    opacity: 1;
    visibility: visible;
  }
}
.DataCard_icn {
  width: 64px;
  height: 64px;
  border-radius: 50%;
  box-shadow: #e7e7e7 7px 6px 19px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: auto;
  margin-top: -52px;
  background: #fff;
}
.inner_DataCard {
  border-radius: 8px;
  border: 1px solid #000;
  padding: 20px 20px 25px 20px;
  transition: 0.3s ease-in-out;
}
.inner_DataCard:hover {
  box-shadow: #000 6px 6px 0;
}
.inner_DataCard p {
  font-size: 18px;
  margin-top: 20px;
  font-weight: 300;
  margin-bottom: 0;
}
.market_tab_blk .video-container {
  border-radius: 30px;
}
.market_tab_blk > .container-fluid > .row > *:nth-child(1) {
  padding-left: 13vw;
  padding-right: 2vw;
}
.market_tab_blk > .container-fluid > .row > *:nth-child(2) {
  padding-left: 2vw;
  padding-right: 6vw;
}
/**************end market_tab_blk**************/

/**************start client_slider_blk**************/
.client_slider_blk .title_blk {
  margin-bottom: 60px;
}
.client_slider_blk .title_blk h2 {
  color: #fff;
  font-weight: 700;
  letter-spacing: 1px;
}
.client_slider_blk {
  overflow: hidden;
}
.cardLogo {
  border-radius: 8px;
  border: 1px solid rgba(255, 255, 255, 0.16);
  height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 13px;
  margin-left: 13px;
}
.slick-dots li button:before {
  color: #fff;
}
.slick-dots li.slick-active button:before {
  color: var(--main_color);
}
.clnt-btn {
  margin-top: 60px;
}
a.btn-white {
  display: inline-block;
  border: transparent 1px solid;
  padding: 14px 20px;
  border-radius: 8px;
  color: #000;
  background: #fff;
  font-weight: 500;
}
a.btn-white:hover {
  border-color: #e9e9e9;
  background: transparent;
  color: #e9e9e9;
}

.client_slider_blk .slick-arrow {
  border-radius: 50%;
  width: 60px;
  height: 60px;
  border: rgba(255, 255, 255, 0.16) 1px solid;
}
.client_slider_blk .slick-prev {
  left: -65px;
}
.client_slider_blk .slick-next {
  right: -65px;
}
.client_slider_blk .slick-next:before {
  background: url(../images/arrow_right.png) no-repeat;
}
.client_slider_blk .slick-prev:before {
  background: url(../images/arrow_left.png) no-repeat;
}
.client_slider_blk .slick-next:before,
.client_slider_blk .slick-prev:before {
  content: "";
  background-position: center center;
  width: 14px;
  height: 12px;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  position: absolute;
}
/**************end client_slider_blk**************/

/**************start our_work**************/
.our_work {
  overflow: hidden;
}
.work_card {
  margin: 0 15px;
  border: 1px solid #f5f5f5;
  position: relative;
  overflow: hidden;
  transition: 0.4s ease-in-out;
}
.work_card a {
  width: 70px;
  height: 70px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  color: #fff;
  background: #000;
  position: absolute;
  bottom: -200px;
  left: 50%;
  transform: translateX(-50%);
  transition: 0.4s ease-in-out;
  box-shadow: #000000a6 2px 7px 24px;
  rotate: -20deg;
}
.work_card:hover a {
  bottom: 7.5rem;
}
.work_card a:hover {
  background: var(--main_color);
  box-shadow: #038aba6b 2px 7px 24px;
  rotate: -20deg;
}
.work_card:hover {
  border-color: #000;
}

.portfolio_btn {
  margin-top: 60px;
}
.portfolio_btn a {
  display: flex;
  align-items: center;
  border-radius: 24px;
  border: 1px solid rgba(0, 0, 0, 0.04);
  background: #fff;
  justify-content: center;
  box-shadow: 13px 14px 12.2px 0px rgba(0, 0, 0, 0.03);
  width: fit-content;
  margin: auto;
  padding: 10px 15px 10px 25px;
  gap: 30px;
  font-size: 16px;
  color: #000;
}
.portfolio_btn a:hover {
  gap: 15px;
}
.portfolio_btn a i {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background: #000;
  color: #fff;
  rotate: -37deg;
  transition: 0.4s ease-in-out;
}
.portfolio_btn a:hover i {
  rotate: 0deg;
  background: var(--main_color);
}
/**************end our_work**************/

/**************start address_blk**************/
.add-tabs-container ul {
  margin-bottom: 55px;
}
.add-tabs-container ul li:nth-child(2) {
  margin-left: 23.5rem;
}
.add-tabs-container ul li:last-child {
  margin-top: -3rem;
}
.add-tabs-container .tabContent .desc {
  display: none;
  animation: fadeIn 0.5s ease-in-out;
}
.add-tabs-container .tabContent .desc.active {
  display: block;
}
.address_card li {
  background: #fafbfc;
  padding: 40px 35px;
  position: relative;
  cursor: pointer;
  width: fit-content;
  margin-bottom: 20px;
  width: 22rem;
}
.address_card h2 {
  font-size: 40px;
  font-weight: 300;
  letter-spacing: 1px;
  display: flex;
  align-items: center;
  gap: 30px;
  margin-bottom: 35px;
  position: relative;
}
.address_card h2:after {
  position: absolute;
  content: "";
  width: 6px;
  height: 100%;
  background: #000;
  left: -35px;
  top: 0;
}
.address_card h2 i {
  color: var(--main_color);
  border: 0.5px solid #d1dade;
  width: 60px;
  height: 60px;
  background: #fbfcfd;
  box-shadow: 4px 4px 30px 0px rgba(0, 0, 0, 0.15);
